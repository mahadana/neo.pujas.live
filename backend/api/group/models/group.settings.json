{
  "kind": "collectionType",
  "collectionName": "groups",
  "info": {
    "name": "group",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "timezone": {
      "type": "string",
      "required": true,
      "default": "UTC"
    },
    "events": {
      "type": "component",
      "repeatable": true,
      "component": "event.group-event"
    }
  }
}
