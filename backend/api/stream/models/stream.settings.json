{
  "kind": "collectionType",
  "collectionName": "streams",
  "info": {
    "name": "stream",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "monastery": {
      "model": "monastery"
    },
    "streamUrl": {
      "type": "string"
    },
    "previousStreamsUrl": {
      "type": "string"
    },
    "embeddable": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
